<!-- views/login.ejs -->
<!doctype html>
<html>
<head>
    <title>Node Authentication</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"> <!-- load bootstrap css -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> <!-- load fontawesome -->
    <style>
        body        { padding-top:80px; }
    </style>
</head>
<body>
<div class="container">

<div class="col-sm-6 col-sm-offset-3">

    <h1><span class=""></span>RA</h1>

    <!-- show any messages that come back with authentication -->
    <% if (this.message && message.length > 0) { %>
        <div class="alert alert-danger"><%= message %></div>
    <% }%>
    <% if (this.ra) { %>
            <p>
                <strong>id</strong>: <%= ra._id %><br>
                <strong>tipo</strong>: <%= ra.tipo %><br>
                <strong>ruta</strong>: <%= ra.ruta %>
                <strong>patron</strong>: <%= ra.patron %>
            </p>
        <% } %>
    <% } %>

    <hr>

    <a href="/ra/">All RA</a>
    <p>Or go <a href="/Profile">Profile</a>.</p>
</div>

</div>

</body>